    "st=input(\"Enter the frame: \")\n",
    "count=0\n",
    "res=\" \"\n",
    "for i in st:\n",
    "    if i=='0' and count<5:\n",
    "        res+='0'\n",
    "        count+=1\n",
    "    elif i==' ':\n",
    "        pass\n",
    "    else:\n",
    "        res+=i \n",
    "        count=0\n",
    "    if count==5:\n",
    "        res+='1'\n",
    "        count=0\n",
    "print(\"Frame after bit stuffing: \",res)"
